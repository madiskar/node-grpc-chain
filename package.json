{
  "name": "@mdkr/grpc-chain",
  "version": "0.4.0",
  "description": "Enables the use of middleware for Node gRPC servers",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "repository": "git@github.com:madiskar/node-grpc-tools.git",
  "author": "Madis Kariler <madis.kariler@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "prepublish": "yarn compile",
    "compile": "tsc -b lib",
    "lint": "eslint './**/*.{js,ts,tsx}' --fix",
    "test": "mocha -r ts-node/register lib/**/*.spec.ts",
    "test-docker": "./test-docker.sh",
    "coverage": "yarn compile && nyc yarn test",
    "coverage-docker": "./coverage-docker.sh"
  },
  "publishConfig": {
    "access": "public"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "eslint --fix"
    ],
    "*.json": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-commit": "git update-index --again"
    }
  },
  "dependencies": {},
  "peerDependencies": {
    "@grpc/grpc-js": "^1.1.3",
    "google-protobuf": "^3.12.2"
  },
  "devDependencies": {
    "@grpc/grpc-js": "^1.1.3",
    "@types/chai": "^4.2.12",
    "@types/google-protobuf": "^3.7.2",
    "@types/mocha": "^8.0.0",
    "@types/node": "^14.0.26",
    "@typescript-eslint/eslint-plugin": "^3.7.1",
    "@typescript-eslint/parser": "^3.7.1",
    "chai": "^4.2.0",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "google-auth-library": "^6.0.5",
    "google-protobuf": "^4.0.0-rc.2",
    "grpc-tools": "^1.9.1",
    "grpc_tools_node_protoc_ts": "^4.1.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "mocha": "^8.0.1",
    "nyc": "^15.1.0",
    "prettier": "^2.0.5",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  }
}
