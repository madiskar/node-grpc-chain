{
  "name": "@mdkr/grpc-chain",
  "version": "0.2.0",
  "description": "Enables the use of interceptors (middleware) for Node gRPC servers",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "repository": "git@github.com:madiskar/node-grpc-tools.git",
  "author": "Madis Kariler <madis.kariler@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "prepublish": "yarn compile",
    "compile": "tsc -b lib",
    "lint": "eslint './**/*.{js,ts,tsx}' --fix"
  },
  "publishConfig": {
    "access": "public"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "eslint --fix"
    ],
    "*.json": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-commit": "git update-index --again"
    }
  },
  "dependencies": {},
  "peerDependencies": {
    "google-protobuf": "^3.12.2",
    "grpc": "^1.24.3"
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/google-protobuf": "^3.7.2",
    "@types/mocha": "^7.0.2",
    "@types/node": "^14.0.13",
    "@typescript-eslint/eslint-plugin": "^3.2.0",
    "@typescript-eslint/parser": "^3.2.0",
    "chai": "^4.2.0",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "google-protobuf": "^3.12.2",
    "grpc": "^1.24.3",
    "grpc-tools": "^1.9.0",
    "grpc_tools_node_protoc_ts": "^4.0.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.10",
    "mocha": "^8.0.1",
    "prettier": "^2.0.5",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5"
  }
}
